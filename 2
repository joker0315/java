import java.util.Scanner;
public class hi3 {
public static void main(String[] args){
Scanner scan= new Scanner(System.in);
	System.out.println("출력할 항의 개수: ");
 int x = scan.nextInt();
	int[] s=new int[x];
	s[0]= 0;
	s[1] = 1;
	System.out.printf("%d %d ",s[0],s[1]);
	for(int i = 2; i < s.length; i++)
	{s[i]= s[i-1] + s[i-2];
	System.out.printf("%d ", s[i]);	
	}		
	


	
	
}
}
